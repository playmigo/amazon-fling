{
  "name": "amazon-fling",
  "version": "1.3.4",
  "summary": "Easily integrate the Amazon Fling SDK to discover and control your FireTV in your app.",
  "description": "Discovering and controlling your FireTV requires the Amazon Fling SDK",
  "homepage": "https://github.com/playmigo/amazon-fling",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Playmigo": "info@playmigo.com"
  },
  "source": {
    "git": "https://github.com/playmigo/amazon-fling.git",
    "tag": "1.3.4"
  },
  "platforms": {
    "ios": "9.0"
  },
  "ios": {
    "frameworks": [
      "CFNetwork",
      "Security",
      "SystemConfiguration"
    ],
    "vendored_frameworks": [
      "Frameworks/AmazonFling.framework",
      "Frameworks/Bolts.framework"
